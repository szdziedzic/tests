{
  "cli": {
    "version": ">= 14.3.1",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "ios": {
        "image": "latest"
      },
      "android": {
        "image": "latest",
        "cache": {
          "paths": ["../../.gradle/caches"]
        }
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "channel": "preview"
    },
    "preview-simulator": {
      "extends": "preview",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
