{
  "cli": {
    "version": ">= 14.3.1",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "ios": {
        "image": "latest",
        "resourceClass": "large"
      },
      "android": {
        "image": "latest",
        "resourceClass": "large"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "channel": "preview"
    },
    "preview-simulator": {
      "extends": "preview",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "channel": "production"
    },
    "custom-build": {
      "extends": "preview-simulator",
      "config": "build.yml"
    },
    "staging-ci": {
      "bun": "1.1.38",
      "autoIncrement": true,
      "developmentClient": false,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "environment": "preview",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "preview"
      },
      "withoutCredentials": true
    }
  },
  "submit": {
    "production": {}
  }
}
